// Online C++ compiler to run C++ program online
#include <iostream>
#include<queue>
using namespace std;

class Node{
    public:
    int data;
    Node* left;
    Node* right;
    
    Node(int d){
        this->data=d;
        this->left = NULL;
        this->right= NULL;
    }
};
Node* InsertIntoTree(Node* &root,int d){
    if(root == NULL){
        root=new Node(d); 
        return root;
    }
    if(d >root->data){
        root->right=InsertIntoTree(root->right,d);
    }else{
                root->left=InsertIntoTree(root->left,d);

    }
    return root;
}
    void levelOrderTraveral(Node* root){
        queue<Node*> q;
        q.push(root);
        q.push(NULL);
        
        while(!q.empty()){
            Node* temp =q.front();
            q.pop();
            
            if(temp==NULL){
                //the old level is completed traversal
                cout<<endl;
                if(!q.empty()){
                    q.push(NULL);
                }
                
            }
            else{
                cout << temp->data << " ";
                if (temp->left){
                    q.push(temp->left);
                }
                if (temp->right){
                    q.push(temp->right);
                }
            }
        }
    } 

void TakeData(Node* &root){
    int data;
    cin >> data;
    while (data!= -1){
        root = InsertIntoTree(root,data);
        cin >> data; 
    }
}
int main() {
   Node* root =NULL;
cout << "enter the data for tree" << endl;
  TakeData(root);
  levelOrderTraveral(root);
    return 0;
}
